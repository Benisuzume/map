#!/usr/bin/env lua

local Tasks = require ('map.tasks')

return Tasks.run {
	arguments = { ... },

	tasks = {
		'check'
	},

	help = [[
# map check

Parse Jass scripts and validate their syntax.

## Usage
    check [options]
    check <configuration>

## Options
    --version    Displays the version number.
    --help       Displays this help message.]]
}
